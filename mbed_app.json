{
    "target_overrides": {
        "*": {
            "target.macros_remove": [
                "CONFIG_GPIO_AS_PINRESET"
            ],
            "target.macros_add":[
                "MBED_TICKLESS",
                "DEVICE_LPTICKER",
                "DEVICE_SLEEP",
                "CONFIG_NFCT_PINS_AS_GPIOS",
                "NRFX_GPIOTE_CONFIG_NUM_OF_LOW_POWER_EVENTS=8",
                "MBED_CONF_TARGET_TICKLESS_FROM_US_TICKER=0"
            ],
            "target.device_has_remove": [
                "ITM",
                "SERIAL",
                "SERIAL_ASYNCH",
                "SERIAL_FC"
            ],
            "target.console-uart": false,
            "target.console-uart-flow-control": null,
            "platform.cpu-stats-enabled": true,
            "target.lse_available": true,
            "events.use-lowpower-timer-ticker": true,
            "platform.fatal-error-auto-reboot-enabled": true,
            "platform.crash-capture-enabled": true,
            "platform.error-reboot-max": 3
        }
    }
}